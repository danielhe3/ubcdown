	pages = {1007--1014},
	file = {Full Text PDF:/Users/danielhe/Zotero/storage/QSLHUKWJ/Marchal-Sommé et al. - 2007 - Dendritic Cells Accumulate in Human Fibrotic Inter.pdf:application/pdf},
