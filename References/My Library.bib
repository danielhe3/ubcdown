	pmcid = {PMC7138412},
	keywords = {Humans, Idiopathic Pulmonary Fibrosis, Adult, Aged, Male, Middle Aged, Mucin-5B, Matrix Metalloproteinase 7, Gene Expression Profiling, Transcriptome, Case-Control Studies, Oligonucleotide Array Sequence Analysis, Phenotype, Cilia, Interstitial Fibrosis, RNA},
