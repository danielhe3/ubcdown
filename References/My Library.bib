	keywords = {IPF, Metalloproteinases, MMP, Lung fibrosis, Matrisome},
	pages = {23},
	file = {Full Text PDF:/Users/danielhe/Zotero/storage/LGC5QDCG/Pardo et al. - 2016 - Role of matrix metalloproteinases in the pathogene.pdf:application/pdf},
}

