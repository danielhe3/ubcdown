	journal = {American Journal of Physiology-Lung Cellular and Molecular Physiology},
	author = {Hung, Chi F. and Wilson, Carole L. and Chow, Yu-Hua and Liles, Wayne C. and Gharib, Sina A. and Altemeier, William A. and Schnapp, Lynn M.},
	month = apr,
