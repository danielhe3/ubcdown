	keywords = {Humans, Pulmonary Fibrosis, Killer Cells, Natural, Case-Control Studies, Bronchoalveolar Lavage Fluid, Immunophenotyping, Antibodies, Monoclonal, CD56 Antigen, Receptors, IgG, Th2 Cells},
	pages = {541--545},
