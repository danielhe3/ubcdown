	pages = {53--63},
	file = {PubMed Central Full Text PDF:/Users/danielhe/Zotero/storage/ARLKCQFN/Hanmandlu et al. - Transcriptomic and Epigenetic Profiling of Fibrobl.pdf:application/pdf},
