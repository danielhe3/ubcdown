	journal = {Cytokine},
	author = {Galati, Domenico and De Martino, Marina and Trotta, Annamaria and Rea, Gaetano and Bruzzese, Dario and Cicchitto, Gaetano and Stanziola, Anna Agnese and Napolitano, Maria and Sanduzzi, Alessandro and Bocchino, Marialuisa},
