	pmcid = {PMC4694094},
	pages = {1644--1651},
	file = {PubMed Central Full Text PDF:/Users/danielhe/Zotero/storage/XXXAM2XU/Fernández Pérez et al. - 2013 - Identifying an Inciting Antigen Is Associated With.pdf:application/pdf},
}

