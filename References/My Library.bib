	pmid = {21974901},
	pmcid = {PMC3199230},
	keywords = {Humans, Idiopathic Pulmonary Fibrosis, Adult, Aged, Female, Male, Middle Aged, Lung, Cohort Studies, Regression Analysis, Predictive Value of Tests, Gene Expression Profiling, Sensitivity and Specificity, Phenotype, ROC Curve, Cluster Analysis, Lung Transplantation, Area Under Curve, Bayes Theorem, Tissue Donors},
