	journal = {American Journal of Respiratory and Critical Care Medicine},
	author = {Newton, Chad A. and Zhang, David and Oldham, Justin M. and Kozlitina, Julia and Ma, Shwu-Fan and Martinez, Fernando J. and Raghu, Ganesh and Noth, Imre and Garcia, Christine Kim},
	month = aug,
