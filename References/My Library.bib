	pmid = {35522243},
	keywords = {Humans, Idiopathic Pulmonary Fibrosis, idiopathic pulmonary fibrosis, interstitial lung disease, Lung, Lung Diseases, Interstitial, Animals, Mice, Arthritis, Rheumatoid, Epigenesis, Genetic, Janus kinase 2, Janus Kinase 2, rheumatoid arthritis, STAT3 transcription factor, STAT5 transcription factor},
