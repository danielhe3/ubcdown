	keywords = {idiopathic pulmonary fibrosis (IPF), lung resident mesenchymal stem cells (LR-MSCs), myofibroblast differentiation, NF-κB signaling, tumor necrosis factor-α (TNF-α)},
	pages = {2409--2419},
}

