	keywords = {Humans, Idiopathic Pulmonary Fibrosis, Disease Progression, Biomarkers, Matrix Metalloproteinase 7, Prospective Studies},
	pages = {2101612},
	file = {Full Text:/Users/danielhe/Zotero/storage/A2XNS9CE/Khan et al. - 2022 - A systematic review of blood biomarkers with indiv.pdf:application/pdf},
}

